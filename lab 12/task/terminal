fum@LABVM:~$ cd infosec/brute-force-server/
fum@LABVM:~/infosec/brute-force-server$ nano gen_personal_wordlist.py
fum@LABVM:~/infosec/brute-force-server$ nano victim.txt
fum@LABVM:~/infosec/brute-force-server$ python3 gen_personal_wordlist.py victim.txt > personal_passwords.txt
fum@LABVM:~/infosec/brute-force-server$ sort -u personal_passwords.txt -o personal_passwords.txt
fum@LABVM:~/infosec/brute-force-server$ wc -l personal_passwords.txt
